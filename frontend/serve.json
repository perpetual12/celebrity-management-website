{
  "rewrites": [
    { "source": "**", "destination": "/index.html" }
  ],
  "redirects": [
    { "source": "/admin-login", "destination": "/index.html", "type": 200 },
    { "source": "/login", "destination": "/index.html", "type": 200 },
    { "source": "/dashboard", "destination": "/index.html", "type": 200 },
    { "source": "/celebrities", "destination": "/index.html", "type": 200 },
    { "source": "/admin", "destination": "/index.html", "type": 200 }
  ],
  "headers": [
    {
      "source": "**",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}
